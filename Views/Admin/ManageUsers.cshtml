@model List<Demo_1670.Models.User>

@{
    ViewData["Title"] = "Manage Users";
}
<a asp-action="Dashboard" class="btn btn-secondary mt-3">Back to Dashboard</a>
<h2>Manage Users</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Username</th>
            <th>Role</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Role</td>
                <td>
                    <a asp-action="Detail" asp-route-username="@user.Username">View</a>
                   
                </td>
            </tr>
        }
    </tbody>
</table>
